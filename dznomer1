
double s, a;

s = double.Parse(Console.ReadLine());

if (Math.Log(s, 3) % 1 == 0 || Math.Log(s, 3) % 1 >= 0.9999999)
{
    Console.WriteLine("Yes");


    a = Math.Log(s, 3) % 1;

    Console.WriteLine(a);
}
else
{
    Console.WriteLine("No");
    a = Math.Log(s, 3) % 1;

    Console.WriteLine(a);
}    
